<?xml version="1.0"?>
<launch>
    <param name="i2c_bus" value="1"/>
    <param name="i2c_address" value="0x68"/>
    <!-- raspberrypi RX is GPI14 -->
    <param name="interrupt_gpio_pin" value="4"/>
    <!-- 250Hz -->
    <param name="gyro_dlpf_frequency" value="0"/>
    <!-- 460Hz -->
    <param name="accel_dlpf_frequency" value="0"/>
    <!-- +/- 250deg/sec -->
    <param name="gyro_fsr" value="0"/>
    <!-- +/- 2g -->
    <param name="accel_fsr" value="0"/>
    <!--   The maximum allowable sensor data rate, in Hz. 
    Data rate is normally calculated as half of the accel/gyro DLPF frequency (nyquist criterion). 
    This parameter allows maximum cap on the data rate, regardless of the DLPF frequency. -->
    <param name="max_data_rate" value="8000"/>
    <node pkg="driver_mpu9250" type="driver_mpu9250_rpi" name="driver_mpu9250">
    </node>
</launch>
